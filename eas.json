{
  "cli": {
    "version": ">= 13.0.0"
  },
  "build": {
    "devclient": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "image": "latest"
      },
      "env": {
        "NODE_ENV": "development"
      }
    },
    "production": {
      "workflow": "managed",
      "distribution": "store",
      "android": {
        "buildType": "app-bundle",
        "image": "latest"
      },
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "submit": {
    "production": {
      "android": {
        "strategy": "slowlane"
      }
    }
  }
}
