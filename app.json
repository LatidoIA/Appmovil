{
  "expo": {
    "name": "Latido IA",
    "slug": "latido",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./adaptive-icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": false,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Se usa para compartir tu ubicación en emergencias.",
        "NSMicrophoneUsageDescription": "Se usa para detección por voz en emergencias."
      }
    },
    "android": {
      "package": "com.latidoia.app",
      "edgeToEdgeEnabled": true,
      "permissions": [
        "VIBRATE",
        "CALL_PHONE",
        "RECORD_AUDIO",
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "plugins": [
      "expo-health-connect",
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "minSdkVersion": 26
          }
        }
      ]
    ],
    "web": { "favicon": "./favicon.png" },
    "extra": { "eas": { "projectId": "2ac93018-3731-4e46-b345-6d54a5502b8f" } }
  }
}
